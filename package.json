{
  "name": "hungryhippo",
  "version": "1.0.0",
  "description": "creates feeds where there were none before",
  "license": "GPL-3.0-only",
  "author": "Huey <hello@huey.xyz>",
  "main": "dist/app.js",
  "scripts": {
    "babel-node": "babel-node --extensions \".ts\" src/app.ts",
    "build": "babel src -d dist --extensions \".ts\" --copy-files --source-maps",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "start": "node dist/app.js",
    "start:dev": "nodemon -e ts --exec npm run babel-node",
    "start:prod": "node dist/app",
    "test": "echo \"No tests\" && exit 0"
  },
  "lint-staged": {
    "*.{ts,d.ts}": [
      "tsc --allowSyntheticDefaultImports --jsx react --noEmit --esModuleInterop --moduleResolution node --skipLibCheck --resolveJsonModule --downlevelIteration --lib ESNEXT,ES2020.String,dom"
    ],
    "*.{ts,js,json}": [
      "eslint --fix --ignore-pattern"
    ],
    "package.json": [
      "npx sort-package-json"
    ]
  },
  "dependencies": {
    "axios": "github:hueyy/axios",
    "cheerio": "^1.0.0-rc.9",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "feed": "^4.2.2",
    "form-data": "^4.0.0",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "playwright-chromium": "^1.11.1",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/eslint-plugin": "^7.13.16",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@types/express": "^4.17.11",
    "@types/node": "^15.6.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "eslint": "7.27.0",
    "eslint-plugin-disable": "^2.0.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.7.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-unicorn": "33.0.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "typescript": "^4.2.4"
  }
}