{
  "name": "hungryhippo",
  "version": "1.0.4",
  "description": "creates feeds where there were none before",
  "license": "AGPL-3.0-only",
  "author": "Huey <hello@huey.xyz>",
  "main": "dist/app.js",
  "scripts": {
    "babel-node": "babel-node --extensions \".ts\" src/app.ts",
    "build": "babel src -d dist --extensions \".ts\" --copy-files --source-maps",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "start": "node dist/app.js",
    "start:dev": "nodemon -e ts --exec npm run babel-node",
    "start:prod": "node dist/app",
    "test": "echo \"No tests\" && exit 0"
  },
  "lint-staged": {
    "*.{ts,d.ts}": [
      "tsc-files --noEmit"
    ],
    "*.{ts,js,json}": [
      "eslint --fix --ignore-pattern"
    ],
    "package.json": [
      "npx sort-package-json"
    ]
  },
  "dependencies": {
    "axios": "^0.27.2",
    "axios-cache-interceptor": "^0.10.7",
    "cheerio": "^1.0.0-rc.12",
    "dayjs": "^1.11.4",
    "express": "^4.18.1",
    "feed": "^4.2.2",
    "form-data": "^4.0.0",
    "helmet": "^5.1.1",
    "morgan": "^1.10.0",
    "playwright-chromium": "^1.24.2",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/eslint-plugin": "^7.18.10",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.13",
    "@types/node": "^18.11.2",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "dotenv": "^16.0.1",
    "eslint": "8.21.0",
    "eslint-plugin-disable": "^2.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-sonarjs": "^0.14.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "tsc-files": "^1.1.3",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=v16.1.0"
  }
}
