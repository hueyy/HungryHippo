{
  "name": "hungryhippo",
  "version": "1.0.0",
  "description": "creates feeds where there were none before",
  "license": "GPL-3.0-only",
  "author": "Huey <hello@huey.xyz>",
  "main": "dist/app.js",
  "scripts": {
    "babel-node": "babel-node --extensions \".ts\" src/app.ts",
    "build": "babel src -d dist --extensions \".ts\" --copy-files --source-maps",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "start": "node dist/app.js",
    "start:dev": "nodemon -e ts --exec npm run babel-node",
    "start:prod": "node dist/app",
    "test": "echo \"No tests\" && exit 0"
  },
  "lint-staged": {
    "*.{ts,d.ts}": [
      "tsc-files --noEmit"
    ],
    "*.{ts,js,json}": [
      "eslint --fix --ignore-pattern"
    ],
    "package.json": [
      "npx sort-package-json"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.10",
    "dayjs": "^1.10.6",
    "express": "^4.17.1",
    "feed": "^4.2.2",
    "form-data": "^4.0.0",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "playwright-chromium": "^1.13.1",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.15.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "dotenv": "^10.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-disable": "^2.0.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.9.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unicorn": "^35.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "tsc-files": "^1.1.2",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=v16.1.0"
  }
}
